{
  "workflowVersion": "4.0.0",
  "integration": "canvas-central",
  "figmaSync": "conduit",
  "tokenFrames": [
    {
      "name": "Tokens / Colors",
      "prefixes": ["color"]
    },
    {
      "name": "Tokens / Typography",
      "prefixes": ["font"]
    },
    {
      "name": "Tokens / Spacing",
      "prefixes": ["space", "size", "radius", "shadow", "opacity", "z"]
    }
  ],
  "breakpoints": [
    {
      "name": "mobile",
      "label": "Mobile",
      "width": 390
    },
    {
      "name": "tablet",
      "label": "Tablet",
      "width": 768
    },
    {
      "name": "laptop",
      "label": "Laptop",
      "width": 1280
    },
    {
      "name": "desktop",
      "label": "Desktop",
      "width": 1440
    }
  ],
  "exchangeFiles": {
    "codeToCanvas": "handoff/code-to-canvas.json",
    "canvasToCode": "handoff/canvas-to-code.json",
    "canvasToCodeTemplate": "handoff/canvas-to-code.template.json"
  }
}
